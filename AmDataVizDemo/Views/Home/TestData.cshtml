@{
    ViewData["Title"] = "Test Data";
}
<script src="~/js/test_data.js" asp-append-version="true" defer="defer"></script>

<!-- Start Breadcrumbbar -->
<div class="breadcrumbbar">
    <div class="row align-items-center">
        <div class="col-md-8 col-lg-8">
            <h1 class="page-title">@ViewData["Title"]</h1>
            <div class="breadcrumb-list">
                <ol class="breadcrumb">
                    <li class="breadcrumb-item"><a href="~/Home/Dashboard">Home</a></li>
                    <li class="breadcrumb-item active" aria-current="page"><a href="~/Home/TestData">@ViewData["Title"]</a></li>
                </ol>
            </div>
        </div>
        <div class="col-md-4 col-lg-4">
            <div class="widgetbar">
                <button class="btn btn-primary"><i class="ri-refresh-line mr-2"></i>Refresh</button>
            </div>
        </div>
    </div>
</div>
<!-- End Breadcrumbbar -->
<!-- Start Contentbar -->
<div class="contentbar">
    <div class="grid-row">
        <div class="card col-12">
            <ul class="nav nav-tabs" id="jobGraphTabs" role="tablist">
                <li class="nav-item">
                    <a class="nav-link active" id="job-table-tab" data-toggle="tab" href="#job-table" role="tab" aria-controls="job table" aria-selected="true">Job Table</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" id="job-bar-chart-tab" data-toggle="tab" href="#job-bar-chart" role="tab" aria-controls="job bar chart" aria-selected="false">Job Bar Chart</a>
                </li>
            </ul>
            <div class="tab-content" id="myTabContent">
                <div class="tab-pane fade show active" id="job-table" role="tabpanel" aria-labelledby="job-table-tab">
                    <table class="dataTable display" id="job-test-data-table">
                        <thead>
                            <tr>
                                <th>Job Name</th>
                                <th>Number of DataPoints</th>
                            </tr>
                        </thead>
                        <tbody>
                                <tr>
                                    <td></td>
                                    <td></td>
                                </tr>
                        </tbody>
                    </table>
                </div>
                <div class="tab-pane fade" id="job-bar-chart" role="tabpanel" aria-labelledby="job-bar-chart">
                    <div id="job-bart-chart-container" style="width:100%; height:400px;"></div>
                </div>
            </div>
        </div>
    </div>
    <div class="grid-row mt-3">
        <div class="col-12 card">
            <h2>All Test Reports</h2>
            <table class="dataTable display" id="all-test-data-table">
                <thead>
                    <tr>
                        <th>Job Name</th>
                        <th>Supplier Name</th>
                        <th>Step Name</th>
                        <th>Equipment Type</th>
                        <th>KPI Name</th>
                        <th>KPI Value</th>
                        <th>KPI Units</th>
                    </tr>
                </thead>
                <tbody>
                    <tr>
                        <td></td>
                        <td></td>
                        <td></td>
                        <td></td>
                        <td></td>
                        <td></td>
                        <td></td>
                    </tr>
                </tbody>
            </table>
        </div>
    </div>
</div>
<!-- End Contentbar -->